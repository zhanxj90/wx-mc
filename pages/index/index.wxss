.map{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 86rpx;
}
#map{
  width: 100%;
  height: 100%;
}

.tabBar {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 86rpx;
}
.tabBar>view{
  flex: 1;
  text-align: center;
  line-height: 86rpx;
  color: #fff;
  font-size: 36rpx
}
.publish{
  background-color: #ff9700;
}
.search{
  background-color: #03bbd5;
}